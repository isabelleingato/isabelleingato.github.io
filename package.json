{
  "name": "iingato-personal-site",
  "private": true,
  "description": "Isabelle Ingato's personal site",
  "version": "1.0.0",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.js\"",
    "lint": "eslint ./**/*.js",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "profile": "gatsby build --profile",
    "debug": "gatsby develop --inspect-brk",
    "deploy": "gatsby build && gh-pages -d public -b public-deploy",
    "test": "jest",
    "lighthouse": "lhci collect && lhci assert"
  },
  "dependencies": {
    "bootstrap": "^4.5.2",
    "gatsby": "^2.24.26",
    "gatsby-plugin-manifest": "^2.4.30",
    "gatsby-plugin-offline": "^3.2.28",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-theme-ui": "^0.3.0",
    "gatsby-plugin-typography": "^2.5.10",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-remark": "^2.8.28",
    "identity-obj-proxy": "^3.0.0",
    "node-sass": "^4.14.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^17.0.1",
    "react-typography": "^0.16.19",
    "theme-ui": "^0.3.1",
    "typography": "^0.16.19",
    "typography-theme-fairy-gates": "^0.16.19",
    "typography-theme-funston": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "gh-pages": "^3.1.0",
    "jest": "^26.6.3",
    "prettier": "2.0.5",
    "@lhci/cli": "0.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/isabelleingato/isabelleingato.github.io"
  }
}
